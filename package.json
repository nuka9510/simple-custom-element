{
  "name": "@nuka9510/simple-custom-element",
  "version": "1.4.4",
  "description": "simple custom element util for web front-end",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.min.mjs",
      "require": "./dist/cjs/index.min.cjs"
    },
    "./plugin": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/esm/plugin/index.min.mjs",
      "require": "./dist/cjs/plugin/index.min.cjs"
    },
    "./@types/component": {
      "types": "./@types/component.d.ts"
    },
    "./@types/interceptor": {
      "types": "./@types/interceptor.d.ts"
    },
    "./@types/plugin": {
      "types": "./@types/plugin.d.ts"
    },
    "./@types/plugin/util-action": {
      "types": "./@types/plugin/util-action.d.ts"
    },
    "./@types/register": {
      "types": "./@types/register.d.ts"
    }
  },
  "files": [
    "@types",
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -r -f dist && tsc && webpack --config webpack.config.cjs",
    "postversion": "git push && git push --tag && npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuka9510/simple-custom-element.git"
  },
  "keywords": [
    "util",
    "web",
    "front-end",
    "custom-element",
    "web-component",
    "module",
    "CommonJS",
    "ES Module",
    "cjs",
    "esm"
  ],
  "author": "nuka9510",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuka9510/simple-custom-element/issues"
  },
  "homepage": "https://github.com/nuka9510/simple-custom-element",
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/webpack": "^5.28.5",
    "ts-node": "^10.9.2",
    "webpack": "^5.99.5",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@nuka9510/js-util": "^1.2.6"
  }
}
